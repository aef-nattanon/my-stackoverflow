{
	"name": "my-stackoverflow",
	"displayName": "My Stackoverflow",
	"description": "vscode extension for stackoverflow code recommendation",
	"version": "0.1.0",
	"publisher": "aefnattanon",
	"engines": {
		"vscode": "^1.59.0"
	},
	"icon": "ui/logo-512.png",
	"repository": {
		"type": "git",
		"url": "https://github.com/aef-nattanon/my-stackoverflow.git"
	},
	"license": "MIT",
	"homepage": "https://github.com/aef-nattanon/my-stackoverflow/blob/master/README.md",
	"bugs": {
		"url": "https://github.com/aef-nattanon/my-stackoverflow/issues",
		"email": "aef.nattanon@gmail.com"
	},
	"keywords": [
		"Stackoverflow"
	],
	"author": {
		"name": "aef.nattanon"
	},
	"categories": [
		"Education"
	],
	"activationEvents": [
		"onCommand:my-stackoverflow.myView"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"keybindings": [
			{
				"command": "my-stackoverflow.myView",
				"key": "ctrl+k f"
			}
		],
		"commands": [
			{
				"command": "my-stackoverflow.myView",
				"title": "Open My StackOverflow"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "yarn run package",
		"compile": "webpack",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"test-compile": "tsc -p ./",
		"test-watch": "tsc -watch -p ./",
		"pretest": "yarn run test-compile && yarn run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.2.2",
		"@types/node": "14.x",
		"@types/vscode": "^1.59.0",
		"@typescript-eslint/eslint-plugin": "^4.26.0",
		"@typescript-eslint/parser": "^4.26.0",
		"eslint": "^7.27.0",
		"glob": "^7.1.7",
		"mocha": "^8.4.0",
		"ts-loader": "^9.2.2",
		"typescript": "^4.3.2",
		"vscode-test": "^1.5.2",
		"webpack": "^5.38.1",
		"webpack-cli": "^4.7.0"
	},
	"dependencies": {
		"fs": "^0.0.1-security",
		"path": "^0.12.7"
	}
}
